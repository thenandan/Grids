{
    "name": "thenandan/grids",
    "description": "Grids for Laravel 5+ frameworks",
    "keywords": [
        "grid",
        "tables",
        "html",
        "laravel",
        "database",
        "eloquent",
        "bootstrap",
        "laravel-5",
        "laravel5"
    ],
    "config": {
        "secure-http": false,
        "preferred-install": {
            "*": "dist"
        },
        "github-protocols": [
            "git",
            "https"
        ],
        "discard-changes": true,
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "homepage": "https://github.com/thenandan/grids",
    "type": "laravel-library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Keshari Nandan",
            "email": "thenandan4@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravelcollective/html": "v6.*",
        "illuminate/support": ">=4.2",
        "nayjest/builder": "~2"
    },
    "suggest": {
        "laravelcollective/html": "Required to work with Laravel 5.X",
        "maatwebsite/excel": "Required to work with Excel export component"
    },
    "autoload": {
        "psr-4": {
            "TheNandan\\Grids\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "ThaNandan\\Grids\\GridsServiceProvider"
            ]
        }
    }
}
